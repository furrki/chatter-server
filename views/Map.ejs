<html lang="en">
<%- include('parts/head.ejs') %>
<body class="index" id="page-top" cz-shortcut-listen="true">

    <% if(isLogged){ %>
        <%- include('parts/header.ejs') %>
    <% } else { %>
        <%- include('parts/public_header.ejs') %>
    <% }  %>


    <section class="container featured-news ">
        <div class="row">
            <div class="trending-product col-md-12 col-xs-12 col-lg-12 col-sm-12 ">
                <div id="googleMap" style="width:100%;height:650px;"></div>
            </div>
        </div>
    </section>
    <script src="/assets/js/mapStyles.js"></script>
    <script>



        function myMap() {
            var mapProp= {
                center: new google.maps.LatLng(51.508742,-0.120850),
                zoom:8,
                maxZoom:20,
                minZoom: 7,
                styles: window.mapStyles,
            };
            var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(showPosition);
            } else {
                x.innerHTML = "Geolocation is not supported by this browser.";
            }
            function showPosition(pos){
                map.setCenter(new google.maps.LatLng(pos.coords.latitude, pos.coords.latitude))
            }
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBOmKk11CsvJ1_RA6yi4H3_0znRd4zzwW8&callback=myMap"></script>
    <%- include('parts/footer.ejs') %>
</body>
</html>
